{
	"name": "deepsquare.io",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"postbuild": "pnpm dlx svelte-sitemap --domain https://deepsquare-io.github.io/website",
		"test": "pnpm run test:integration && pnpm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@picocss/pico": "1.5.11",
		"@playwright/test": "1.40.1",
		"@sveltejs/adapter-static": "3.0.1",
		"@sveltejs/enhanced-img": "^0.1.8",
		"@sveltejs/kit": "2.3.2",
		"@sveltejs/vite-plugin-svelte": "3.0.1",
		"@tsparticles/engine": "^3.1.0",
		"@tsparticles/plugin-canvas-mask": "^3.1.0",
		"@tsparticles/plugin-polygon-mask": "^3.1.0",
		"@tsparticles/slim": "^3.1.0",
		"@tsparticles/svelte": "^3.0.0",
		"@types/eslint": "8.56.2",
		"@typescript-eslint/eslint-plugin": "6.18.1",
		"@typescript-eslint/parser": "6.18.1",
		"autoprefixer": "10.4.16",
		"eslint": "8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.35.1",
		"material-symbols": "0.14.5",
		"postcss": "8.4.33",
		"prettier": "3.2.2",
		"prettier-plugin-svelte": "3.1.2",
		"sass": "1.69.7",
		"svelte": "4.2.8",
		"svelte-check": "3.6.3",
		"svelte-inview": "^4.0.2",
		"svelte-markdown": "^0.4.1",
		"svelte-sitemap": "^2.6.0",
		"tailwindcss": "3.4.1",
		"tslib": "2.6.2",
		"typescript": "5.3.3",
		"vite": "5.0.11",
		"vitest": "1.2.0"
	},
	"type": "module"
}
